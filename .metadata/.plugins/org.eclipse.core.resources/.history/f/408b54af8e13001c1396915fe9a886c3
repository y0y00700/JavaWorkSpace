
public class InstanceOfOper {

	class Person{
		String name;
		String id;
		
		public Person(String name) {
			this.name=name;
		}
	}
	
	class Student extends Person{
		String name;
		String id;
		
		public Student (String name) {
			super(name);
		}
	}
	
	public class UpcastingEx{
		public static void main(String args[]) {
			Person p;
			Student s = new Student("¿Ã¿ÁπÆ");
		}
	}
}
